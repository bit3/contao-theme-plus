{
  "name":"bit3/contao-theme-plus",
  "description":"Theme+ for Contao OpenSource CMS",
  "keywords":["contao", "theme", "css", "js", "stylesheet", "javascript", "assets"],
  "type":"contao-module",
  "homepage":"http://bit3.de",
  "license":"LGPL-3.0+",
  "authors":[
    {
      "name":"Tristan Lins",
      "email":"tristan.lins@bit3.de",
      "homepage":"http://bit3.de",
      "role":"Author"
    },
    {
      "name":"David Molineus",
      "email":"david.molineus@netzmacht.de",
      "homepage":"http://netzmacht.de",
      "role":"Developer"
    }
  ],
  "support":{
    "email":"info@bit3.de",
    "issues":"https://github.com/netzmacht/contao-theme-plus/issues",
    "wiki":"https://github.com/netzmacht/contao-theme-plus/wiki",
    "irc":"irc://irc.freenode.org/contao.dev",
    "source":"https://github.com/netzmacht/contao-theme-plus"
  },
  "require":{
    "php":">=5.3",
    "contao/core":">=3,<3.6-dev",
    "contao-community-alliance/composer-plugin": "~2",
    "netzmacht/contao-assetic":"~1",
    "ikimea/browser":"dev-master",
    "mobiledetect/mobiledetectlib": "2.*",
    "contao-community-alliance/meta-palettes":"~1"
  },
  "require-dev":{
    "squizlabs/php_codesniffer":"1.*",
    "bit3/php-coding-standard":"@dev"
  },
  "replace": {
    "contao-legacy/theme_plus": "self.version"
  },
  "autoload":{
    "psr-0": {
      "Bit3\\Contao\\ThemePlus": "src",
      "ThemePlus": "deprecated/"
    }
  },
  "extra":{
    "contao": {
      "sources":{
        "contao":"system/modules/theme-plus"
      },
      "runonce":[
        "runonce/upgrade_to_contao3_2.php"
      ]
    },
    "branch-alias": {
      "dev-master": "4.3.x-dev",
      "dev-develop": "5.0.x-dev"
    }
  }
}
